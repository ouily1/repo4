node{
   stage('SCM Checkout'){
     git 'https://github.com/ouily1/repo4'
   }
   stage('Compile-Package'){
      // Get maven home path
      def mvnHome =  tool name: 'maven-3', type: 'maven'   
      "${mvnHome}/bin/mvn package"
   }
   
   stage('SonarQube Analysis') {
        def mvnHome =  tool name: 'maven-3', type: 'maven'
        withSonarQubeEnv('sonar') { 
         "${mvnHome}/bin/mvn sonar:sonar"
        }
    }
}
